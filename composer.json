{
   "name": "rovota/core",
   "version": "1.0.0",
   "type": "library",
   "description": "A modern PHP framework designed for a painless development experience.",
   "keywords": ["framework", "php", "rovota", "modern", "web"],
   "homepage": "https://github.com/Rovota/Core",
   "license": "MIT",
   "authors": [
      {
         "name": "Software Department",
         "email": "developers@rovota.com"
      },
      {
         "name": "Rovota Community",
         "homepage": "https://rovota.com/community"
      }
   ],
   "support": {
      "email": "support@rovota.com",
      "docs": "https://rovota.gitbook.io/core"
   },
   "require": {
      "php-64bit": "^8.2.0",
      "ext-curl": "*",
      "ext-exif": "*",
      "ext-fileinfo": "*",
      "ext-mbstring": "*",
      "ext-openssl": "*",
      "ext-pdo": "*",
      "ext-sodium": "*",
      "ext-zip": "*",
      "dflydev/dot-access-data": "^3.0",
      "envms/fluentpdo": "^2.2",
      "guzzlehttp/guzzle": "^7.7",
      "league/commonmark": "^2.4",
      "league/flysystem": "^3.15",
      "league/flysystem-read-only": "^3.15",
      "monolog/monolog": "^3.4",
      "phlak/semver": "^4.1",
      "phpmailer/phpmailer": "^6.8",
      "phpseclib/phpseclib": "^3.0",
      "spomky-labs/otphp": "^11.2"
   },
   "require-dev": {
      "league/flysystem-aws-s3-v3": "^3.15",
      "league/flysystem-sftp-v3": "^3.15"
   },
   "suggest": {
      "league/flysystem-aws-s3-v3": "Allows usage of S3 compatible services as file storage.",
      "league/flysystem-sftp-v3": "Allows usage of SFTP file storage.",
      "ext-redis": "Allows you to use Redis-based caching.",
      "ext-apcu": "Allows you to use APCu-based caching.",
      "ext-imagick": "Required for being able to process/modify images."
   },
   "autoload": {
      "psr-4": {
         "Rovota\\Core\\": "src/"
      }
   },
   "conflict": {
      "phpmailer/phpmailer": "<6.0",
      "league/flysystem": "<3.0"
   },
   "config": {
      "sort-packages": true,
      "optimize-autoloader": true
   },
   "replace": {
      "symfony/polyfill-php80": "*",
      "symfony/polyfill-mbstring": "*",
      "ralouphie/getallheaders": "*"
   }
}